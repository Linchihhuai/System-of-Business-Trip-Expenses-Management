  method TRAJET_SPECIFIQU_DELETE_ENTITY.

  DATA return TYPE STANDARD TABLE  OF bapireturn1.
  DATA ls_return LIke LINE OF return.

  DATA lt_trajet TYPE STANDARD TABLE OF zhr_trajet.
  DATA wa_trajet LIKE LINE OF lt_trajet.

  DATA lt_sous_trajet TYPE STANDARD TABLE OF zhr_sous_trajet.
  DATA wa_sous_trajet LIKE LINE OF lt_sous_trajet.

  DATA lt_keys      type /iwbep/t_mgw_tech_pairs.
  DATA ls_key       type /iwbep/s_mgw_tech_pair.
  DATA lv_ID TYPE zhr_trajet-pernr.

  lt_keys = IO_TECH_REQUEST_CONTEXT->GET_KEYS( ).
  READ TABLE lt_keys WITH KEY name = 'ID' INTO ls_key.
  lv_ID = ls_key-value.

  SELECT SINGLE * FROM zhr_trajet INTO CORRESPONDING FIELDS OF wa_trajet WHERE  ENDDA > sy-datum AND Id = lv_ID.
  wa_trajet-endda = sy-datum.
  wa_trajet-etat ='SUP'.

 UPDATE zhr_trajet FROM wa_trajet .

    IF sy-subrc <> 0.
        RAISE EXCEPTION TYPE /IWBEP/CX_MGW_BUSI_EXCEPTION
        EXPORTING textid = /IWBEP/CX_MGW_BUSI_EXCEPTION=>business_error
        message = ls_return-message.
     ENDIF.

 SELECT * FROM zhr_sous_trajet INTO TABLE lt_sous_trajet WHERE  ENDDA > sy-datum AND Fk_trajet = lv_ID.

   LOOP at lt_sous_trajet into wa_sous_trajet.
     wa_sous_trajet-endda = sy-datum.
     wa_sous_trajet-etat = 'SUP'.
     UPDATE zhr_sous_trajet FROM wa_sous_trajet.

   ENDLOOP.

    IF sy-subrc <> 0.
        RAISE EXCEPTION TYPE /IWBEP/CX_MGW_BUSI_EXCEPTION
        EXPORTING textid = /IWBEP/CX_MGW_BUSI_EXCEPTION=>business_error
        message = ls_return-message.
     ENDIF.

  endmethod.
